  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

	pod ‘Alamofire’
	pod ‘Kingfisher’
	pod ‘SVProgressHUD’
	pod ‘ObjectMapper’
	pod ‘AlamofireObjectMapper’

  target 'League ChampsTests' do

    inherit! :search_paths
    # Pods for testing
  end

  target 'League ChampsUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
